.weui-cells {
  background-color: #b57ee2c2;

  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
}

.weui-cell__hd {
  align-items: center;
  justify-content: center;
  background-color: white;
  border: 1px solid #b57ee2c2;
  border-radius: 20rpx;
}

.choose {
  font-size: large;
}

.icon0{
  background-size: 100% 100%;
  height: 350rpx;
  width: 350rpx;
  background-image: url(data:image/png;base64,);
}

.icon1 {
  background-size: 100% 100%;
  height: 350rpx;
  width: 350rpx;
  background-image: url(data:image/png;base64,);
}

.weui-btn {
  /*进入微办公小程序*/
  height: 80rpx;
  width: 60%;
  margin-top: 60rpx;
